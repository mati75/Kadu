index 99f0913..96aaf85 100644
--- a/plugins/mprisplayer_mediaplayer/mpris_controller.h
+++ b/plugins/mprisplayer_mediaplayer/mpris_controller.h
@@ -94,9 +94,6 @@ public:
 
 	QList<TrackInfo> getTrackList() const;
 
